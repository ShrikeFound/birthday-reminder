{
  "rules": {
    "users":{
      "$user_id":{
        ".read":"$user_id === auth.uid",
        ".write":"$user_id === auth.uid"
      }
    },
    "birthdays":{
      ".read":"auth !== null",
      ".write":"auth !== null",
      "$user_id":{
        ".read":"$user_id === auth.uid",
        ".write":"$user_id === auth.uid"
      }
    },
    ".read": false,
    ".write": false
  }
}